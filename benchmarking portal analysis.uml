  <Requirements:Requirement xmi:id="_v7pf8MWtEe2m-vko_vR7ZA" base_NamedElement="_v7WlAMWtEe2m-vko_vR7ZA" id="R9.2.2_A" text="A futó benchmarkok nézetén átszódjon, hogy az adott mérés hol tart éppen." base_Class="_v7WlAMWtEe2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_suXPIMWuEe2m-vko_vR7ZA" base_NamedElement="_suOFMMWuEe2m-vko_vR7ZA" id="R1.2.1_B" text="A mérés részeredményei legyenek láthatók a mérés futása közben már, nem csak a legvégén." base_Class="_suOFMMWuEe2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_K1Nt8MWvEe2m-vko_vR7ZA" base_NamedElement="_K1GZMMWvEe2m-vko_vR7ZA" id="R10_A" text="A használt virtuális gépek statisztikái, információi a nem admin felhasználók számára legyenek rejtettek." base_Class="_K1GZMMWvEe2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_hbA2IMWvEe2m-vko_vR7ZA" base_NamedElement="_ha79oMWvEe2m-vko_vR7ZA" id="R12_A" text="Más felhasználók által feltöltött futtatható állományok és source set-ek is legyenek láthatóak minden regisztrált felhasználó számára." base_Class="_ha79oMWvEe2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_JBR1sMWwEe2m-vko_vR7ZA" base_NamedElement="_JBOLUMWwEe2m-vko_vR7ZA" id="R2.3_B" text="Az adott fehasználó által létrehozott entitások (benchmark-ok, futtatható állományok, source set-ek) legyenek az adott felhasználó által törölhetők." base_Class="_JBOLUMWwEe2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_fYvKccWxEe2m-vko_vR7ZA" base_NamedElement="_fYvKcMWxEe2m-vko_vR7ZA" id="R1.1.2_A" text="A felhasználónak legyen lehetősége saját SET fájlt megadni a benchmark konfigurálásánál." base_Class="_fYvKcMWxEe2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_upDS0MWxEe2m-vko_vR7ZA" base_NamedElement="_upBdoMWxEe2m-vko_vR7ZA" id="R9.1_A" text="A korábbi saját mérések egy külön nézeten legyen megtekinthetők." base_Class="_upBdoMWxEe2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_fV5cYMWyEe2m-vko_vR7ZA" base_NamedElement="_fVzVwMWyEe2m-vko_vR7ZA" id="R9.4_B" text="A released Theta verziókról készült benchmarkoknak legyen külön nézet." base_Class="_fVzVwMWyEe2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_t8t0cMW1Ee2m-vko_vR7ZA" base_NamedElement="_t8nGwMW1Ee2m-vko_vR7ZA" id="R9.2.1_B" text="Legyenek csoportosítva az elindított benchmark-ok az alapján, hogy a bejelentkezett felhasználó indította-e el őket, vagy pedig más." base_Class="_t8nGwMW1Ee2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_6E-mwMW1Ee2m-vko_vR7ZA" base_NamedElement="_6E68YMW1Ee2m-vko_vR7ZA" id="R9.2_A" text="Legyen egy nézet, ahol az összes felhasználó által elindított mérés látszódik." base_Class="_6E68YMW1Ee2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_JFjC8cW2Ee2m-vko_vR7ZA" base_NamedElement="_JFjC8MW2Ee2m-vko_vR7ZA" id="R9.3.1_A" text="Az adott nézeten belül a feltöltött futtatható állományok külön legyenek megjelenítve." base_Class="_JFjC8MW2Ee2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_dUHYkMW3Ee2m-vko_vR7ZA" base_NamedElement="_dUCgEMW3Ee2m-vko_vR7ZA" id="R9.3.2_A" text="Az adott nézeten belül a feltöltött source set-ek külön legyenek megjelenítve." base_Class="_dUCgEMW3Ee2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_JXOW4cW4Ee2m-vko_vR7ZA" base_NamedElement="_JXOW4MW4Ee2m-vko_vR7ZA" id="R3_B" text="A portál kínáljon tetszőleges számú kiválasztott mérésre statisztikai lehetőségeket, melyek a mérések tulajdonságait vetik össze." base_Class="_JXOW4MW4Ee2m-vko_vR7ZA"/>
  <Requirements:Requirement xmi:id="_lIiWUMXnEe2m-vko_vR7ZA" base_NamedElement="_lIcPsMXnEe2m-vko_vR7ZA" id="R6_B" text="Legyen a portálnak mobil nézete is." base_Class="_lIcPsMXnEe2m-vko_vR7ZA"/>
